Member
    approved Bool
    name Text
    homecity Text
    email Text
    hyyMember Bool        -- Is member of hyy
    birthdate Day Maybe
    biologicalGender Bool Maybe -- True is male, False female
    recruitedBy Text
    desc Text Maybe
    memberSince UTCTime default=CURRENT_TIME
    UniqueMember email

User
    username Text
    UniqueUsername username
    password ByteString
    email Text
    verified Bool
    verifyKey Text
    resetPasswordKey Text
    autoPublishImages Bool default=0 -- Allow all images of you be by default public
    deriving Typeable

BlogPost
    ident Text
    title Text
    authors [Text]
    tags [Text]
    log EditLog
    markdown Markdown
    rendered Html
    UniqueBlogPost ident

Album
    public Bool default=1
    date UTCTime
    ident Text
    title Text
    author Text
    UniqueAlbum author ident

Image
    public Bool default=1
    date UTCTime
    album AlbumId
    nth Int
    title Text
    author Text
    contentType ContentType
    file FilePath
    fileExt String
    UniqueImage album nth

PersonInImage
    img ImageId
    user UserId
    publishable Bool Maybe -- Nothing if not acked by user, or Just "can publish"
